
@model Wediary.Models.ProjectManager.ProjectModel;
<div class="container">
    <h2 class="col-md-12">Tworzenie Projektu</h2>
    <form asp-controller="TableManager" asp-action="SaveChanges" method="post">
        <div class="form-group row col-lg-12">
            <label class="col-sm-4 col-form-label text-right">Nazwa Projektu</label>
            <div class="col-sm-6">
                <input asp-for="Name" asp type="text" class="form-control" id="inputName" placeholder="Nazwa Projektu">
            </div>
        </div>
        <div class="form-group row col-md-6">
            <button type="submit" class="btn btn-primary">Dodaj</button>
            <a class="btn btn-warning" asp-controller="TableManager" asp-action="Index">Powróć do Listy</a>
        </div>
    </form>
</div>